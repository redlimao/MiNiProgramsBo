/**collect.wxss**/
.con{width: 100%;display: flex;flex-wrap: wrap;padding: 0 10rpx;box-sizing: border-box;}
    .c-item{width: 345rpx;margin: 0 10rpx;}
        .c-img{width: 345rpx;height: 518rpx;background-color: #ccc;}
        .c-txt{text-align: center;line-height: 62rpx;font-size: 30rpx;color: #2C2C2C;}
.menu{height: 90rpx;width: 100%;box-sizing: border-box;align-items: center;display: flex; justify-content: space-around;}
    .m-item{position: relative;}
        .m-txt{color: #2C2C2C;font-size: 30rpx;line-height: 66rpx;position: relative;z-index: 2;}
        .m-bg{position: absolute;left: 0;bottom: 15rpx;background-color:#FDCF44;width: 100%;height: 15rpx;}
.empty{text-align: center;padding: 200rpx 0;color:#9C9C9C;font-size: 28rpx;}