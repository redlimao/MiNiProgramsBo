/* pages/main/main.wxss */
.container{
  padding-bottom:120rpx;
}
.banner{
  width:100%;
  height:300rpx;
  position:relative;
}
.bg{
  width:100%;
  height:300rpx;
}
.flowers{
  display:block;
  position:absolute;
  right:20rpx;
  bottom:20rpx;
  padding:0 10rpx;
  font-size:22rpx;
  color:#fff;
  background:rgba(0,0,0,0.5);
  height:48rpx;
  line-height:48rpx;
  border-radius:30rpx;
}
/* ===基本信息=== */
.message{
  width:100%;
  height:340rpx;
  position:relative;
  background:#fff;
  margin-bottom:20rpx;
}
.message .photo{
  width:110rpx;
  height:110rpx;
  border-radius:50%;
  border:2rpx solid #fff;
  position:absolute;
  left:0;
  right:0;
  margin:0 auto;
  top:-55rpx;
  box-shadow: 0 0 20rpx rgba(0,0,0,0.5);
}
.message-top{
  padding-top:70rpx;
  margin-bottom:30rpx;
  position:relative;
}
.message-top .name{
  display:block;
  font-size:36rpx;
  color:rgb(73,70,71);
  margin:0 auto;
  text-align:center;
}
.message .tags{
  display:block;
  position:absolute;
  right:20rpx;
  height:49rpx;
  border-radius:50rpx;
  background:#fff;
  border:1rpx solid rgb(255,127,174);
  width:128rpx;
  top:58rpx;
}
.message .tags.nans{
  border:1rpx solid #3894FF;
  color:#3894FF;
}
.message .tags.nans .tags-text{
  color:#3894FF;
}
.video-icon{
  width:32rpx;
  height:26rpx;
  float:left;
  margin-top:11rpx;
  margin-left:20rpx;
  margin-right:10rpx;
}
.tags-text{
  font-size:26rpx;
  color:rgb(255,127,174);
  line-height:48rpx;
  display:block;
  float:left;
}
.message .self{
  display:block;
  float:right;
  margin-right:20rpx;
  width:126rpx;
  height:48rpx;
  text-align:center;
  line-height:48rpx;
  font-size:26rpx;
  color:#FF7FAE;
  border:1rpx solid #FF7FAE;
  border-radius:50rpx;
}
/* ===相册=== */
.album{
  width:100%;
  height:160rpx;
  display:flex;
  justify-content:space-around;
  margin-bottom:20rpx;
}
.album .album-list{
  width:160rpx;
  height:160rpx;
  position:relative;
}
.album .album-list .img-list{
  width:160rpx;
  height:160rpx;
  border-radius:6rpx;
}
.album .album-list .mask-list{
  width:160rpx;
  height:160rpx;
  position:absolute;
  left:0;
  top:0;
}
/* ===基本资料=== */
.info-box{
  background:#fff;
  padding:20rpx;
  margin-bottom:20rpx;
}
.info-title{
  font-size:30rpx;
  height:30rpx;
  border-left:6rpx solid #FF7FAE;
  color:#FF7FAE;
  margin-bottom:30rpx;
  padding-left:10rpx;
  font-weight: bold;
  line-height:30rpx;
}
.info-title.nans{
 border-left:6rpx solid #3894FF;
 color:#3894FF;
}
.list{
  width:100%;
  height:90rpx;
  border-bottom:1rpx solid #eee;
  display:flex;
  justify-content:space-between;
}
.list-txt{font-size:30rpx;color:rgb(173,168,166);line-height:90rpx;}
.list-texts{font-size:30rpx;color:rgb(73,70,71);line-height:90rpx;}
/* ===择偶条件=== */
.ou-box{
  width:710rpx;
  padding:0 20rpx;
  background:#fff;
  margin-bottom:20rpx;
  overflow:hidden;
}
.tiao-box{
  width:100%;
  border-top:1rpx solid #eee;
  overflow:hidden;
}
.ou-box .info-title {
  margin-top:20rpx;
}
.tiao-box .open{
  display:block;
  width:280rpx;
  height:70rpx;
  margin:30rpx auto;
}
/* ===我的标签=== */
.qian-box{
  width:710rpx;
  padding:0 20rpx;
  overflow:hidden;
  margin-bottom:20rpx;
  background:#fff;
  padding-bottom:20rpx;
}
.qian-box .info-title {
  margin-top:20rpx;
}
.qian-tags{
  width:100%;
  border-top:1rpx solid #eee;
  overflow:hidden;
}
.qian-list{
  width:130rpx;
  height:48rpx;
  text-align:center;
  line-height:48rpx;
  background:#f8f8f8;
  color:rgb(73,70,71);
  border-radius:30rpx;
  font-size:26rpx;
  float:left;
  margin:0 6rpx;
  margin-top:20rpx;
}
/* ===送花=== */
.flower{
  width:100%;
  height:100rpx;
  position:fixed;
  left:0;
  bottom:0;
  background:#fff;
  overflow:hidden;
  border-top:1rpx solid #eee;
}
.guan{
  width:30%;
  float:left;
  overflow:hidden;
}
.guan-icon{
  width:30rpx;
  height:30rpx;
  display:block;
  float:left;
  margin-top:35rpx;
  margin-left:45rpx;
  margin-right:10rpx;
}
.guan-text{
  display:block;
  float:left;
  line-height:100rpx;
  font-size:30rpx;
  color:rgb(73,70,71);
}
.huan{
  width:40%;
  text-align:center;
  line-height:30rpx;
  font-size:30rpx;
  color:rgb(73,70,71);
  border-left:1rpx solid #eee;
  border-right:1rpx solid #eee;
  margin-top:35rpx;
  float:left;
}
.song{
  width:180rpx;
  height:60rpx;
  border-radius:4rpx;
  text-align:center;
  line-height:60rpx;
  color:#fff;
  font-size:30rpx;
  background:rgb(255,127,174);
  margin-top:18rpx;
  float:right;
  margin-right:20rpx;
}
.song-texts{
  font-size:24rpx;
}
.song.nans{
  background:#3894FF;
}
button:after{
  border:none;
}
button{
    color:rgb(73,70,71);
    width:100%;
    height:100%;
    font-size:30rpx;
    line-height:30rpx;
    margin:0;
    background:#fff;
}
.empty{
  width:670rpx;
  height:405rpx;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  top:900rpx;
}
.empty .empty-icon{
  width:339rpx;
  height:405rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
/* ===送花弹框==== */
.flower-box{
  width:100%;
  height:100%;
  position:fixed;
  left:0;
  top:0;
}
.flower-box .flower-mask{
  width:100%;
  height:100%;
  position:fixed;
  left:0;
  top:0;
  background:rgba(0,0,0,0.5);
}
.flower-song{
  width:560rpx;
  height:500rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
  background:#fff;
  border-radius:20rpx;
}
.flower-song .flower-icon{
  width:146rpx;
  height:146rpx; 
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  top:-73rpx;
  border-radius:50%;
}
.flower-text{
  display:block;
  width:100%;
  text-align:center;
  font-size:44rpx;
  color:rgba(73,70,71,1);
  margin-top:90rpx;
  margin-bottom:30rpx;
}
.suan{
  width:250rpx;
  overflow:hidden;
  margin:0 auto;
  margin-bottom:120rpx;
}
.suan .jian{
  display:block;
  float:left;
  width:48rpx;
  height:48rpx;
  text-align:center;
  line-height:48rpx;
  font-size:48rpx;
   color:rgba(173,166,168,1);
}
.suan .inp{
  width:150rpx;
  display:block;
  float:left;
  height:48rpx;
  line-height:48rpx;
  color:#FF7FAE;
  font-size:48rpx;
  border-bottom:1rpx solid rgba(238,238,238,1);
  text-align: center;
}
.suan .jia{
  display:block;
  float:left;
  width:48rpx;
  height:48rpx;
  text-align:center;
  line-height:48rpx;
  font-size:48rpx;
  color:rgba(255,127,174,1);
}
.btns{
  width:409rpx;
  height:98rpx; 
  margin:0 auto;
}
.song-icon{
  width:409rpx;
  height:98rpx;
}
.flower-ze{
  width:100%;
  text-align:center;
  font-size:22rpx;
  color:rgba(216,210,211,1);
  display:block;
}
.flower-close{
  width:120rpx;
  height:120rpx;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  top:960rpx;
}
.closes-icon{
  width:60rpx;
  height:60rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}

/* =====图片放大====== */
.big-mask{
  width:100%;
  height:100%;
  position:fixed;
  left:0;
  top:0;
  background:rgba(0,0,0,0.5);
}
.big-mask .swipers{
  width:100%;
  height:100%;
}
.big-mask .swipers swiper-item{
  width:100%;
  height:100%;
  display:flex;
  justify-content:space-around;
  flex-direction:column;
}
.banners{
  width:100%;
}
/*===视频===  */
.videos{
  width:100%;
  position:fixed;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
  background:rgba(0,0,0,0.5);
}
.videos video{
  position:fixed;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
.video-close{
  width:120rpx;
  height:120rpx;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  top:1000rpx;
}
.video-close .video-icon{
  width:60rpx;
  height:60rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}