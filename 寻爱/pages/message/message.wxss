/* pages/main/main.wxss */
.banner{
  width:100%;
  height:300rpx;
  position:relative;
}
.huan-bg{
  width:144rpx;
  height:48rpx;
  display:block;
  text-align:center;
  line-height:48rpx;
  background:rgba(0,0,0,0.6);
  color:#fff;
  font-size:22rpx;
  position:absolute;
  right:20rpx;
  bottom:20rpx;
  border-radius:30rpx;
}
.bg{
  width:100%;
  height:300rpx;
}
/* ===基本信息=== */
.message{
  width:100%;
  height:340rpx;
  position:relative;
  background:#fff;
  margin-bottom:20rpx;
}
.message .photo{
  width:110rpx;
  height:110rpx;
  border-radius:50%;
  border:2rpx solid #fff;
  position:absolute;
  left:0;
  right:0;
  margin:0 auto;
  top:-55rpx;
  box-shadow: 0 0 20rpx rgba(0,0,0,0.5);
  overflow:hidden;
  background:#f8f8f8;
}
.photo .camre-icon{
  display:block;
  width:30rpx;
  height:30rpx;
  margin:10rpx auto;
  margin-top:20rpx;
} 
.photo .up-text{
  display:block;
  text-align:center;
  font-size:20rpx;
  color:rgb(255,127,174);
}
.message-top{
  overflow:hidden;
  padding-top:70rpx;
  margin-bottom:30rpx;
}
.message .tags{
  display:block;
  float:right;
  margin-right:20rpx;
  height:48rpx;
  border-radius:50rpx;
  background:#fff;
  border:1rpx solid rgb(255,127,174);
  overflow:hidden;
  width:130rpx;
}
.message .tags.nans{
  border:1rpx solid #3894FF;
  color:#3894FF;
}
.message .tags.nans .tags-text{
  color:#3894FF;
}
.video-icon{
  width:32rpx;
  height:26rpx;
  float:left;
  margin-top:11rpx;
  margin-left:20rpx;
  margin-right:10rpx;
}
.tags-text{
  font-size:26rpx;
  color:rgb(255,127,174);
  line-height:48rpx;
  display:block;
  width:100%;
  text-align:center;
}
.message .self{
  display:block;
  float:right;
  margin-right:20rpx;
  width:126rpx;
  height:48rpx;
  text-align:center;
  line-height:48rpx;
  font-size:26rpx;
  color:#FF7FAE;
  border:1rpx solid #FF7FAE;
  border-radius:50rpx;
}
/* ===相册=== */
.album{
  width:100%;
  height:160rpx;
  display:flex;
  justify-content:space-around;
  margin-bottom:20rpx;
}
.album .album-list{
  width:160rpx;
  height:160rpx;
  position:relative;
  background:#f8f8f8;
}
.album .album-list .img-list{
  width:160rpx;
  height:160rpx;
}
.album .album-list .img-lists{
  width:64rpx;
  height:51rpx;
  display:block;
  margin:0 auto;
  margin-top:40rpx;
  margin-bottom:10rpx;
}
.album .album-list .up-album{
  display:block;
  width:100%;
  text-align:center;
  font-size:20rpx;
  color:rgb(173,168,166);
}
/* ===基本资料=== */
.info-box{
  background:#fff;
  padding:20rpx;
  margin-bottom:20rpx;
}
.info-title{
  font-size:30rpx;
  height:30rpx;
  border-left:6rpx solid #FF7FAE;
  color:#FF7FAE;
  margin-bottom:30rpx;
  padding-left:20rpx;
  font-weight: bold;
  line-height:30rpx;
}
.info-title.nans{
 border-left:6rpx solid #3894FF;
 color:#3894FF;
}
.list{
  width:100%;
  height:90rpx;
  border-bottom:1rpx solid #eee;
  overflow:hidden;
  position:relative;
}
.list-txt{font-size:30rpx;color:rgb(73,70,71);line-height:90rpx;float:left;margin-left:10rpx;}
.list-texts{font-size:30rpx;color:rgb(173,168,166);line-height:90rpx;float:right;}
.list-textss{font-size:30rpx;color:rgb(73,70,71);line-height:90rpx;float:right;}
.inp{
  display:block;
  width:300rpx;
  height:90rpx;
  float:right;
  font-size:30rpx;
  color:rgb(173,168,166);
  line-height:90rpx;
  text-align:right;
}
.phones{
    position: absolute;
    right:45rpx;
    top:0;
    z-index:1;
}
.picker{
  display:block;
  width:400rpx;
  height:90rpx;
  float:right;
  font-size:30rpx;
  color:rgb(173,168,166);
  line-height:90rpx;
  text-align:right;
}
.look-more{float:right;width:24rpx;height:24rpx;margin-top:33rpx;margin-right:10rpx;margin-left:10rpx;}
.bao{
  width:660rpx;
  height:80rpx;
  text-align:center;
  line-height:80rpx;
  font-size:30rpx;
  color:#fff;
  background:#FF7FAE;
  margin:0 auto;
  border-radius:4rpx;
  margin-top:30rpx;
}


/* =====填写资料===== */
.message-mask{
  width:100%;
  height:100%;
  position:fixed;
  left:0;
  top:0;
  z-index:9;
}
.message-mask .bg-mask{
  position: fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.4);
}
.message-mask .mask-inner{
  background:#fff;
  width:100%;
  height:1000rpx;
  padding-top:10rpx;
  padding-bottom:50rpx;
  position:fixed;
  left:0;
  bottom:0;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.mask-inner .mask-close{
  width:710rpx;
  height:50rpx;
  margin:20rpx auto;
  display:flex;
  justify-content:space-between;
}
.mask-close .close-icon{
  display:block;
  width:35rpx;
  height:35rpx;
  margin-top:3rpx;
}
button:after{
  border:none;
}
.mask-close button{
    border:2rpx solid #FF7FAE;
    color:#FF7FAE;
    width:130rpx;
    height:48rpx;
    font-size:30rpx;
    border-radius:30rpx;
    line-height:48rpx;
    margin:0;
    background:#fff;
}
.mask-form{
  width:710rpx;
  margin:0 auto;
  height:900rpx;
  overflow:auto;
  display:none;
}
.mask-form.on{
  display:block;
}
.mask-form .xing-tags{
  width:130rpx;
  height:48rpx;
  display:inline-block;
  text-align:center;
  line-height:48rpx;
  font-size:30rpx;
  background:#f8f8f8;
  border-radius:30rpx;
  margin:0 6rpx;
  margin-bottom:20rpx;
}
.mask-form .xing-tags.actives{
  background:#FF7FAE;
  color:#fff;
}
.mask-form .xing-tagss{
  width:320rpx;
  height:48rpx;
  display:inline-block;
  text-align:center;
  line-height:48rpx;
  font-size:30rpx;
  background:#f8f8f8;
  border-radius:30rpx;
  margin:0 17rpx;
  margin-bottom:20rpx;
}
.mask-form .xing-tagss.actives{
  background:#FF7FAE;
  color:#fff;
}
/*===视频===  */
.videos{
  width:100%;
  position:fixed;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
  background:rgba(0,0,0,0.5);
}
.videos video{
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
.video-close{
  width:120rpx;
  height:120rpx;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  top:1000rpx;
}
.video-close .video-icon{
  width:60rpx;
  height:60rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
.phone{
    height:80rpx;
    line-height:80rpx;
    width:70%;
    position: absolute;
    right:0;
    top:0;
    z-index:2;
    margin:0;
    border:none;
   background:rgba(0,0,0,0);

}
.phone.hide{
  display:none;
}
button:after{
  border:none;
}