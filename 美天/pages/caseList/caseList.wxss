/**导航样式**/
.container{background-color: #f4f4f4;}
.slider-top{padding:0 20rpx;border-top:1rpx solid rgb(238,238,238);box-sizing:border-box;position:relative;background-color: #fff;}
.wrap-c{width: 100%;background-color: #fff;}
    .box-c{display: flex;display: -webkit-box;position: relative;}
        .c-item{height: 80rpx;line-height: 80rpx;padding: 0 10rpx;margin: 0 10rpx;border-bottom: 2rpx solid transparent;text-align: center;font-size: 32rpx;color: #333;box-sizing: border-box;}
        .c-item.on{border-bottom: 2rpx solid #ea541d;color: #ea541d;}

.wrap{width: 100%;background-color: #f4f4f4;padding-top: 20rpx;}
.weekend-list{width: 700rpx;margin: 0 auto;border-radius: 10rpx;background: #fff;padding: 20rpx;box-sizing: border-box;margin-bottom: 20rpx;}
  .weekend-list-img{width: 660rpx;height: 495rpx;}
  .weekend-tit{height: 50rpx;line-height: 50rpx;font-size: 30rpx;color: rgb(51,51,51);overflow: hidden;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 1;}




		
