/* pages/rule/rule.wxss */
page{
  background:#fff;
}
.container{
  background:#fff;
  padding:20rpx;
}
.title{
  width:100%;
  display:block;
  text-align:center;
  line-height:60rpx;
  font-size:38rpx;
  color:#434343;
  font-weight: bold;
}
/* =====底部导航==== */
.botBar{
  width:100%;
  height:100rpx;
  display: flex;
  justify-content:space-between;
  border-top:1rpx solid #eee;
  position:fixed;
  bottom:0;
  left:0;
}
.botBar .tel{
  width:50%;
  text-align: center;
  font-size:30rpx;
  color:#434343;
  line-height:100rpx;
}
.botBar .buy{
  width:50%;
  text-align: center;
  font-size:30rpx;
  color:#fff;
  line-height:100rpx;
  background:#FF7FAE;
}

.intro{width: 100%;}
.intro image{width: 100%;}