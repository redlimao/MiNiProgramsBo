/**app.wxss**/
page{
	background: #f5f5f5;
}
image,text,view{display:block;box-sizing:border-box;}

.container {
  padding-bottom:20rpx;
} 
.container-a {
  padding-bottom:116rpx;
}
.mt-20{margin-top: 20rpx;}
.bb-1{border-bottom: 1rpx solid #ccc;}
.bt-1{border-top: 1rpx solid #ccc;}
input {
    display:block;
    height:0;
    text-overflow:clip;
    overflow:hidden;
    white-space:nowrap;
    font-family:UICTFontTextStyleBody;
    min-height:0;
}

/*===========下划线导航================*/
.pro_nav{
  width:100%;
  height:90rpx;
  display:flex;
  justify-content: space-around;
  background:#fff;
  border-bottom:1rpx solid #eee;
}
.pro_nav .pronav-list{
  height:90rpx;
  line-height:90rpx;
  font-size:30rpx;
  color:rgb(177,174,171);
  overflow:hidden;
  position:relative;
  box-sizing: border-box;
}

.pro_nav .pronav-list.actives{
  color:#4fb94b;
  border-bottom:6rpx solid #4fb94b;
}
/* ========无内容=========*/

.empty{
  width:670rpx;
  height:30rpx;
  position:absolute;
  left:0;
  right:0;
  margin:auto;
  bottom:120rpx;
}
.empty .empty-icon{
  width:100%;
  height:2rpx;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
.empty .empty-text{
  display:block;
  line-height:30rpx;
  color:rgb(177, 174, 171);
  font-size:24rpx;
  text-align:center;
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  margin:auto;
}
