/* pages/couponlist/couponlist.wxss */
.list{padding: 30rpx 30rpx;width: 100%;box-sizing: border-box;}
    .item{display: flex;width: 690rpx;height: 215rpx;position: relative;margin-bottom: 40rpx;}
        .item-img{width: 690rpx;height: 215rpx;position: absolute;top: 0;left: 0;z-index: 2;}
        .infos{width: 690rpx;height: 215rpx;position: absolute;top: 0;left: 0;z-index: 3;display: flex;align-items: center;}
            .info{margin: 0 46rpx 0 38rpx;width: 384rpx;height: 156rpx;border-radius: 5rpx;text-align: center;}
                .title{padding-top: 22rpx;line-height: 64rpx;font-size: 30rpx;color: #333333;}
                .times{line-height: 50rpx;font-size: 24rpx;color: #10474f;}
            .moneys-box{width: 210rpx;}
                .moneys{height: 100rpx;width: 210rpx;color: #10474f;align-items: baseline;display: flex;justify-content: center;}
                    .money-icon{font-size: 40rpx;}
                    .money{font-size: 70rpx;line-height: 100rpx;}
                .txt-money{line-height: 64rpx;font-size: 30rpx;color: #333333;text-align: center;}
        .item-active{position: absolute;top:5rpx;right: 8rpx;font-size: 28rpx;color: #fff;z-index: 3;}

