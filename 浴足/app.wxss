/**app.wxss**/
page{background-color: #f2f2f2;}
view,image,text{display: block;}
.container{width: 100%;height: 100%;display: flex;flex-direction: column;align-items: center;
justify-content: space-between;box-sizing: border-box;padding-bottom: 100rpx;}
.empty{width: 100%;height: 30rpx;line-height: 30rpx;color:rgb(177, 174, 171);font-size:24rpx;text-align:center;position:absolute;left:0;right:0;bottom:200rpx;margin:auto;}
.empty-box1{width: 200rpx;height: 260rpx;transform: translate(-50%, -50%);left: 50%;top: 50%;position: absolute;}
.empty-box{width: 200rpx;height: 260rpx;margin: 0 auto;margin-top: 80rpx;}
.empty-box.mt-100{margin-top:200rpx;}
    .empty-icon{width:200rpx;height: 200rpx;}
    .empty-txt{font-size: 26rpx;text-align: center;color:#9a9a9a;}

/* ======滑动菜单===== s>=slider */
.slider-wrap{padding:0 24rpx;border-top:1rpx solid rgb(238,238,238);background-color: #fff;box-sizing: border-box;width: 100%;}
	.slider-box{height:80rpx;display:flex;white-space:nowrap;}
	.slider-menu{margin: 0 10rpx;height:80rpx;line-height:80rpx;font-size:30rpx;color:rgb(51,51,51);box-sizing: border-box;padding: 0 20rpx;}
    .slider-menu.on{border-bottom:4rpx solid #59d0d0;color: #59d0d0;}

/* 弹窗样式 */
.pop-bg{position: fixed;width: 100%;height: 100%;background-color: rgba(0,0,0,0.5);top: 0;left: 0;z-index: 2;display: none;}
.pop{width: 560rpx;height: 671rpx;position:fixed;top: 50%;left: 50%;z-index: 3;margin-left: -280rpx;margin-top: -335rpx;display: none;}
.pop1{width: 580rpx;padding: 0 20rpx 20rpx 20rpx;box-sizing: border-box;position: fixed;margin: auto;top: 50%; left: 50%;background-color: #fff;transform: translate(-50%,-50%);-webkit-transform: translate(-50%,-50%);}
.pop-bg.on,.pop.on,.pop1.on{display: block;}
    .p-close{width: 60rpx;height: 60rpx;position: absolute;top: 725rpx;left: 250rpx;z-index: 3;}
    .pop-img{position: absolute;top: 0;left: 0;width: 560rpx;height: 671rpx;z-index: 3;}
    .pop-content{position: absolute;top: 0;left: 0;padding-top: 245rpx;height: 426rpx;width: 560rpx;z-index: 4;}
        .p-coupon-view{width: 510rpx;display: flex;justify-content: center;margin:0 auto}
            .p-item{width: 172rpx;height: 172rpx;position: relative;}
                .p-item-img{position: absolute;width: 172rpx;height: 172rpx;top: 0;left: 0;}
                .p-item-con{position: absolute;padding-top: 20rpx;width: 172rpx;height: 152rpx;top: 0;left: 0;z-index: 5;}
                    .p-money{display: flex;height: 80rpx;justify-content: center;align-items: baseline;}
                        .p-money-icon{color: #f9392c;font-size:30rpx}
                        .p-moneys{line-height: 80rpx;font-size: 60rpx;color: #f9392c;}
                    .p-txts{width: 150rpx;padding-top:8rpx;border-top:1rpx dotted #cacaca;color: #666666;font-size: 20rpx;text-align:center;margin:0 auto;}
        .p-texts{padding-top: 29rpx;line-height: 40rpx;text-align: center;font-size: 24rpx;color: #9a9a9a;}
        .p-btn{margin: 0 auto;margin-top: 16rpx;width: 500rpx;height: 80rpx;border-radius: 5rpx;background-color: #59d0d0;color:#fff;font-size: 30rpx;text-align: center;line-height: 80rpx;}
/* 公用尺寸 颜色 图标尺寸*/
.mt-20{margin-top: 20rpx;}
.mt-8{margin-top: 8rpx;}
.mt-32{margin-top: 32rpx;}
.mt-28{margin-top: 28rpx;}
.pt-12{padding-top: 12rpx;}
.pl-20{padding-left: 20rpx;}
.pl-30{padding-left: 30rpx;}
.pt-44{padding-top: 44rpx;}
.bt-1{border-top: 1rpx solid #e1e1e1;}
.bb-1{border-bottom: 1rpx solid #e1e1e1;}
.color-a{color:#f9b000;display: inline;font-weight: bold;}
.color-b{color:#ff695b;}
.color-c{color:#ff6c6c;display: inline;}
.flex-sa{justify-content: space-around;}
.flex-end{justify-content: flex-end;}
.flex-start{justify-content: flex-start;}
.d-box{display: -webkit-box;}
.flex-1{flex: 1;}
.lc-1,.lc-2{overflow: hidden;display: -webkit-box;-webkit-box-orient: vertical;}
.lc-1{-webkit-line-clamp: 1;}
.lc-2{-webkit-line-clamp: 2;}
.fw-b{font-weight: bold;}
.z-index1{z-index: 1;}
.pr{position: relative;}

/* 转入图标尺寸 */
.icon-in{width: 24rpx;height: 24rpx;}
/*个人中心图标尺寸*/
.icon-user{width: 32rpx;height: 32rpx;}
/*详情顶部图标尺寸*/
.icon-a{width: 76rpx;height: 76rpx;}
.icon-b{width: 44rpx;height: 44rpx;}
/*分享图标尺寸*/
.icon-share{width: 44rpx;height: 44rpx;}
/*预约页面帮助图标*/
.help-icon{height: 36rpx;width: 36rpx;}
/*预约页面信息图标*/
.icon-c{width: 36rpx;height: 36rpx;}
/*价格上升图标*/
.price-icon{width: 14rpx;height: 14rpx;}
.star-small{width: 18rpx;height: 18rpx;}
.star-medium{width: 30rpx;height:30rpx;}
.star-big{width: 40rpx;height:40rpx;}


                



